
{% block title %}Utilisateur{% endblock %}

{% block body %}
    <h1>Utilisateur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ utilisateur.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ utilisateur.Nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ utilisateur.Prenom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ utilisateur.Email }}</td>
            </tr>
            <tr>
                <th>Adress</th>
                <td>{{ utilisateur.Adress }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ utilisateur.Tel }}</td>
            </tr>
            <tr>
                <th>Grade</th>
                <td>{{ utilisateur.Grade }}</td>
            </tr>
            <tr>
                <th>UtilisateurRole</th>
                <td>{{ utilisateur.utilisateurRole.value }}</td>
            </tr>
            <tr>
                <th>MedecinSpecilaite</th>
                <td>{{ utilisateur.medecinSpecilaite.value }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_utilisateur_index') }}">back to list</a>

    <a href="{{ path('app_utilisateur_edit', {'id': utilisateur.id}) }}">edit</a>

    {{ include('utilisateur/_delete_form.html.twig') }}
{% endblock %}
